<resources xmlns:tools="http://schemas.android.com/tools">

	<!-- generic -->
	<color name="default_backgroundColor">@android:color/white</color> <!-- generic window background -->
	<color name="default_foregroundColor">#404040</color> <!-- generic window foreground -->
	<color name="default_highlightOnBackgroundColor">@android:color/black</color>
	<color name="default_dimmedOnBackgroundColor">#808080</color>

	<!-- primary -->
	<color name="default_primaryColor">#4251bd</color>
	<color name="default_primaryLightColor" tools:keep="@color/default_primaryLightColor">#797df0</color>
	<color name="default_primaryDarkColor">#00298C</color>
	<color name="default_onPrimaryColor">#EEEEEE</color>
	<color name="default_highlightOnPrimaryColor">@android:color/white</color>

	<!-- secondary -->
	<color name="default_secondaryColor">#FFef2063</color>
	<color name="default_secondaryLightColor" tools:keep="@color/default_secondaryLightColor">#FFff6290</color>
	<color name="default_secondaryDarkColor" tools:keep="@color/default_secondaryDarkColor">#FFb6003a</color>
	<color name="default_onSecondaryColor">#EEEEEE</color>

	<!-- misc fore -->
	<color name="default_primaryForeColor">#ffffff</color>
	<color name="default_secondaryForeColor">#a0a0a0</color>
	<color name="default_tertiaryForeColor">#c0c0c0</color>

</resources>